{
  "name": "dark_mode",
  "short_name": "dark_mode",
  "description": "Turn dark_mode on any site",
  "author": "Stanislav Kondratiev",
  "version": "1.0.1",
  "homepage_url": "https://github.com/technobulka/darkmode-chrome-ext",
  "icons": { "48": "icon48.png", "128": "icon128.png" },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
      "default_title": "dark_mode",
      "default_icon": "icon16.png",
      "default_popup": "popup.html"
  },
  "offline_enabled": true,
  "manifest_version": 2
}
